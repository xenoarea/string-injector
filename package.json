{
  "name": "@xenoarea/string-injector",
  "version": "0.0.7",
  "description": "Little helper to replace placeholders by mapped values into a string",
  "sideEffects": false,
  "type": "module",
  "main": "./src/createInjector.js",
  "exports": "./src/createInjector.js",
  "scripts": {
    "doc": "jsdoc2md --files \"src/createInjector.js\" > doc/createInjector.md",
    "lint": "standard \"src/**/!(*.test).js\"",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xenoarea/string-injector.git"
  },
