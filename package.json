  "author": "XenoArea <xenoarea@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xenoarea/string-injector/issues"
  },
  "homepage": "https://github.com/xenoarea/string-injector#readme",
  "devDependencies": {
    "ava": "^4.2.0",
    "husky": "^8.0.1",
    "jsdoc-to-markdown": "^7.1.1",
    "standard": "^17.0.0"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
